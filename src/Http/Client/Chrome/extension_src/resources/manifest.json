{
  "manifest_version": 2,
  "name": "resourcesRetriever",
  "description": "This extension injects js",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start"
    }
  ]
}